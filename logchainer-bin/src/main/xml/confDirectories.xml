<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<logChainerConf xmlns="http://service.logchainer.ge.ch/">
	<!-- Directories path to adapt to environnement -->

	<listeClientConf>
		<ClientId>ClientA</ClientId>
		<InputDir>../clientExample/input</InputDir>
		<WorkingDir>../clientExample/workingDir</WorkingDir>
		<OutputDir>../clientExample/output</OutputDir>
		<CorruptedFilesDir>../clientExample/corruptedFiles</CorruptedFilesDir>
		<filePattern>
			<separator>_</separator>
			<sortingType>numerical</sortingType>
			<stampPosition>after</stampPosition>
		</filePattern>
		<fileEncoding>UTF-8</fileEncoding>
	</listeClientConf>
</logChainerConf>